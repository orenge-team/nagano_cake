<h3 class="items_index">商品一覧</h3>
<!-- < = link_to " ", new_admin_item_path, class-->

<% @items.each do |item| %>
<table class="table">
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= item.id %></td>
      <td><%= item.name %></td>
      <td><%= item.price %></td>
      <td><%= item.genre %></td>
      <td><%= item.is_active%></td>
    </tr>
  </tbody>
</table>
<% end %>
