<div class='container'>
  <div class='row mt-4'>
  <h3><span style="margin-left:100px">ジャンル編集</span></h3>
  </div>
    <div class='row mt-4'>
      <%= form_with model:@genre, url: admin_genre_path(@genre), local:true do |f| %>
        <table>
              <tr>
                <td><span style="margin-left:30px">ジャンル名</span></td>
                <td><span style="margin-left:80px"><%= f.text_field :name %></span></td>
                <td><span style="margin-left:80px"><%= f.submit '変更を保存' , class: 'btn btn-success'%></span></td>
              </tr>
        </table>
      <% end %>
    </div>rails
</div>